\r\n          </el-form-item>\r\n          <el-form-item prop=\"utype\">\r\n            <el-select\r\n              v-model=\"loginMode.utype\"\r\n              placeholder=\"请选择\"\r\n              style=\"width: 100%\"\r\n            >\r\n              <el-option label=\"学生\" value=\"学生\"> </el-option>\r\n               <el-option label=\"老师\" value=\"老师\"> </el-option>\r\n              <el-option label=\"管理员\" value=\"管理员\"> </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item style=\"width: 100%\">\r\n            <el-button\r\n              type=\"primary\"\r\n              style=\"width: 100%\"\r\n              :loading=\"logining\"\r\n              @click=\"login\"\r\n              >登录</el-button\r\n            >\r\n           \r\n          </el-form-item>\r\n        </el-form>\r\n\r\n       \r\n      </div>\r\n    </div>\r\n  </div>\r\n</el-config-provider>\r\n</template>\r\n\r\n<script>\r\nimport request, { base } from \"../../utils/http\";\r\n\r\nimport { ElConfigProvider } from 'element-plus';\r\nimport zhCn from 'element-plus/lib/locale/lang/zh-cn'\r\nexport default {\r\n  components:{\r\n           [ElConfigProvider.name]: ElConfigProvider\r\n        },\r\n  data() {\r\n       \r\n    return {\r\n      btnLoading: false, //保存按钮加载状态\r\n      formVisible: false, //表单是否可见，默认不可见\r\n      formData: {}, //表单数据\r\n      loginMode: {},\r\n      locale: zhCn,\r\n      rules: {\r\n      \r\n      },\r\n      loading: false,\r\n    };\r\n  },\r\n  created() {\r\n       \r\n  },\r\n  methods: {\r\n      \r\n     \r\n    login() {\r\n      let username = this.loginMode.username;\r\n      let userpw = this.loginMode.userpw;\r\n      let utype = this.loginMode.utype;\r\n      if (!username) {\r\n        this.$message({\r\n          message: \"请输入用户名/工号/学号\",\r\n          type: \"error\",\r\n          //offset: 200,\r\n        });\r\n        return false;\r\n      }\r\n      if (!userpw) {\r\n        this.$message({\r\n          message: \"请输入密码\",\r\n          type: \"error\",\r\n          //offset: 200,  显示的位置\r\n        });\r\n        return false;\r\n      }\r\n         if (!utype) {\r\n        this.$message({\r\n          message: \"请选择用户类型\",\r\n          type: \"error\",\r\n          //offset: 200,\r\n        });\r\n        return false;\r\n      }\r\n      let url = base + \"/ulogin/login\";\r\n      this.loading = true;\r\n        this.loginMode.utype = utype;\r\n        request.post(url, this.loginMode).then((res) => {\r\n        this.loading = false;\r\n        if (res.code == 200) {\r\n          console.log(JSON.stringify(res.resdata));\r\n          sessionStorage.setItem(\"currentUser\", JSON.stringify(res.resdata));\r\n          sessionStorage.setItem(\"utype\", utype);\r\n          this.$router.push(\"/main\");\r\n        } else {\r\n          this.$message({\r\n            message: res.msg,\r\n            type: \"error\",\r\n          });\r\n        }\r\n      });\r\n\r\n      //\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style   scoped>\r\n.login-container {\r\n  /*box-shadow: 0 0px 8px 0 rgba(0, 0, 0, 0.06), 0 1px 0px 0 rgba(0, 0, 0, 0.02);*/\r\n  -webkit-border-radius: 5px;\r\n  border-radius: 5px;\r\n  -moz-border-radius: 5px;\r\n  background-clip: padding-box;\r\n  margin: 180px auto;\r\n  width: 360px;\r\n  height: 320px;\r\n  padding: 35px 35px 15px 35px;\r\n  background: #fff;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  border: 1px solid #eaeaea;\r\n  box-shadow: 0 0 25px #cac6c6;\r\n}\r\n\r\n.title {\r\n  margin: 0px auto 40px auto;\r\n  text-align: center;\r\n  color: #505458;\r\n}\r\n.remember {\r\n  margin: 0px 0px 35px 0px;\r\n}\r\n\r\n.loginbody {\r\n  overflow: scroll;\r\n  overflow-y: hidden;\r\n  overflow-x: hidden;\r\n}\r\n\r\n.login {\r\n  width: 100vw;\r\n  padding: 0;\r\n  margin: 0;\r\n  height: 100vh;\r\n  font-size: 16px;\r\n  background-position: left top;\r\n  background-color: #242645;\r\n  color: #fff;\r\n  font-family: \"Source Sans Pro\";\r\n  position: relative;\r\n  background-image: url(\"../assets/images/login-bg.jpg\");\r\n  background-repeat: no-repeat;\r\n  background-size: 100% 100%;\r\n}\r\n\r\n.mylogin {\r\n  width: 240px;\r\n  height: 280px;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  margin: auto;\r\n  padding: 50px 40px 40px 40px;\r\n  box-shadow: -15px 15px 15px rgba(6, 17, 47, 0.7);\r\n  opacity: 1;\r\n  background: linear-gradient(\r\n    230deg,\r\n    rgba(53, 57, 74, 0) 0%,\r\n    rgb(0, 0, 0) 100%\r\n  );\r\n}\r\n</style>"],"sourceRoot":""}]}