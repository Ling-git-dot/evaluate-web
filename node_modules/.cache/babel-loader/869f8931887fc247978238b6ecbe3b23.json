;AAAA,iBAAG,C,UAAA,CAAH;AAAA,S;;;;OAAxC,CAIK,EAHL,YAAA,CAEC,oBAFD,EAEC;AAFU,QAAA,IAAI,EAAC,SAEf;AAF0B,QAAA,OAAO,EAAE,KAAA,CAAA,UAEnC;AAFgD,QAAA,OAAK,EAAE,QAAA,CAAA;AAEvD,OAFD,E;0BACG;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OADH,E;;AAAA,Q,sBAAA,CAGK,CALP,CAKO,CALP;AAAA,KADe,C;sBA1BjB;AAAA,aAyBU,CAzBV,YAAA,CAyBU,kBAzBV,EAyBU;AAxBP,QAAA,KAAK,EAAE,KAAA,CAAA,QAwBA;AAvBR,QAAA,GAAG,EAAC,aAuBI;AAtBP,QAAA,KAAK,EAAE,KAAA,CAAA,KAsBA;AArBR,uBAAY;AAqBJ,OAzBV,E;0BAME;AAAA,iBAEe,CAFf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC,MAEL;AAFY,YAAA,IAAI,EAAC;AAEjB,WAFf,E;8BACE;AAAA,qBAAgE,CAAhE,YAAA,CAAgE,mBAAhE,EAAgE;4BAA7C,KAAA,CAAA,QAAA,CAAS,IAAoC;;yBAA7C,KAAA,CAAA,QAAA,CAAS,IAAT,GAAa,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAAhE,E,IAAA,E;;AAAA,gB,cAAA,CAAgE,CAAhE;AAAA,a;;;;WADF,CAEe,EACf,YAAA,CAee,uBAff,EAee;AAfD,YAAA,KAAK,EAAC,IAeL;AAfU,YAAA,IAAI,EAAC;AAef,WAff,E;8BACE;AAAA,qBAaY,CAbZ,YAAA,CAaY,oBAbZ,EAaY;4BAZD,KAAA,CAAA,QAAA,CAAS,GAYR;;yBAZD,KAAA,CAAA,QAAA,CAAS,GAAT,GAAY,M;kBAYX;AAXV,gBAAA,WAAW,EAAC,KAWF;AAVV,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAUU,eAbZ,E;kCAKE;AAAA,yBAA6C,CAA7C,YAAA,CAA6C,oBAA7C,EAA6C;AAAlC,oBAAA,KAAK,EAAC,KAA4B;AAAtB,oBAAA,KAAK,EAAC;AAAgB,mBAA7C,CAA6C,G,kBAC7C,YAAA,CAMY,SANZ,EAMY,IANZ,EAMY,WAAA,CALK,KAAA,CAAA,YAKL,EALiB,UAApB,IAAoB,EAAhB;yCADb,YAAA,CAMY,oBANZ,EAMY;AAJT,sBAAA,GAAG,EAAE,IAAI,CAAC,EAID;AAHT,sBAAA,KAAK,EAAE,IAAI,CAAC,IAAL,GAAS,GAAT,GAAkB,IAAI,CAAC,GAGrB;AAFT,sBAAA,KAAK,EAAE,IAAI,CAAC;AAEH,qBANZ,E,IAAA,E;;AAAA,sB,kBAAA,C;mBAMY,CANZ,E;;AAAA,mBAD6C,EAA7C;AAAA,iB;;;;eALF,E;;AAAA,gB,cAAA,CAaY,CAbZ;AAAA,a;;;;WADF,CADe,CAFf;AAAA,S;;;;OANF,E;;AAAA,Q,kBAAA,CAyBU,CAzBV;AAAA,K;;;;GANF,E;;AAAA,I,cAAA,CAyCQ,CAjJV,C","sourcesContent":["<template>\r\n  <section style=\"margin: 10px\">\r\n    <!--工具条-->\r\n    <el-col\r\n      :span=\"24\"\r\n      class=\"toolbar\"\r\n      style=\"padding-bottom: 0px; margin-left: 10px\"\r\n    >\r\n      <el-form :inline=\"true\" :model=\"filters\">\r\n        <el-form-item>\r\n          <el-input\r\n            v-model=\"filters.name\"\r\n            size=\"small\"\r\n            placeholder=\"课程名称\"\r\n          ></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item>\r\n          <el-button type=\"primary\" size=\"small\" @click=\"query\">查询</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"small\"\r\n            @click=\"handleAdd\"\r\n            v-if=\"utype == '管理员'\"\r\n            >新增</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-col>\r\n    <!--列表-->\r\n    <el-table\r\n      :data=\"datalist\"\r\n      border\r\n      stripe\r\n      v-loading=\"listLoading\"\r\n      highlight-current-row\r\n      max-height=\"600\"\r\n      size=\"small\"\r\n      style=\"width: 100%\"\r\n    >\r\n      <el-table-column\r\n        prop=\"name\"\r\n        align=\"center\"\r\n        label=\"课程名称\"\r\n        min-width=\"20%\"\r\n      ></el-table-column>\r\n      <el-table-column prop=\"title\" align=\"center\" label=\"老师\" min-width=\"20%\">\r\n        <template #default=\"scope\">\r\n          {{ scope.row.userinfoVO.name }}\r\n          {{ scope.row.userinfoVO.sno }}</template\r\n        >\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"300\">\r\n        <template #default=\"scope\">\r\n          <el-button\r\n            v-if=\"utype == '管理员'\"\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            @click=\"handleEdit(scope.$index, scope.row)\"\r\n            >编辑</el-button\r\n          >\r\n          <el-button\r\n            v-if=\"utype == '管理员'\"\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.$index, scope.row)\"\r\n            >删除</el-button\r\n          >\r\n\r\n\r\n         <el-button\r\n            v-if=\"utype == '学生'\"\r\n            type=\"success\"\r\n            size=\"mini\"\r\n            @click=\"handlePj(scope.$index, scope.row)\"\r\n            >开始评教</el-button\r\n          >\r\n\r\n\r\n <el-button\r\n            v-if=\"utype == '管理员' || utype == '老师'\"\r\n            type=\"success\"\r\n            size=\"mini\"\r\n            @click=\"handleResult(scope.$index, scope.row)\"\r\n            >评教结果</el-button\r\n          >\r\n\r\n\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"page.currentPage\"\r\n      :page-size=\"page.pageSize\"\r\n      background\r\n      layout=\"total, prev, pager, next, jumper\"\r\n      :total=\"page.totalCount\"\r\n      style=\"float: right; margin: 10px 20px 0 0\"\r\n    >\r\n    </el-pagination>\r\n    <!--表单-->\r\n    <el-dialog\r\n      title=\"课程信息\"\r\n      v-model=\"formVisible\"\r\n      width=\"40%\"\r\n      :close-on-click-modal=\"false\"\r\n    >\r\n      <el-form\r\n        :model=\"formData\"\r\n        ref=\"formDataRef\"\r\n        :rules=\"rules\"\r\n        label-width=\"100px\"\r\n      >\r\n        <el-form-item label=\"课程名称\" prop=\"name\">\r\n          <el-input v-model=\"formData.name\" style=\"width: 50%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"老师\" prop=\"tid\">\r\n          <el-select\r\n            v-model=\"formData.tid\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 50%\"\r\n          >\r\n            <el-option label=\"请选择\" value=\"\"> </el-option>\r\n            <el-option\r\n              v-for=\"item in userinfoList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name + ' ' + item.sno\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"formVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" :loading=\"btnLoading\" @click=\"save\"\r\n            >确 定</el-button\r\n          >\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </section>\r\n</template>\r\n<script>\r\nimport request, { base } from \"../../../utils/http\";\r\nexport default {\r\n  name: \"Kcinfo\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      filters: {\r\n        //列表查询参数\r\n        name: \"\",\r\n        tid: \"\",\r\n      },\r\n      page: {\r\n        currentPage: 1, // 当前页\r\n        pageSize: 8, // 每页显示条目个数\r\n        totalCount: 0, // 总条目数\r\n      },\r\n      userinfoList: \"\",\r\n      listLoading: false, //列表加载状态\r\n      btnLoading: false, //保存按钮加载状态\r\n      datalist: [], //表格数据\r\n      formVisible: false, //表单是否可见，默认不可见\r\n      formData: {}, //表单数据\r\n      ureadonly: false,\r\n      utype: \"\",\r\n      rules: {},\r\n      addrules: {\r\n        name: [{ required: true, message: \"请输入课程名称\", trigger: \"blur\" }],\r\n        tid: [{ required: true, message: \"请输入老师\", trigger: \"blur\" }],\r\n      },\r\n      modifyrules: {\r\n        name: [{ required: true, message: \"请输入课程名称\", trigger: \"blur\" }],\r\n        tid: [{ required: true, message: \"请输入老师\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    let utype = sessionStorage.getItem(\"utype\");\r\n    this.utype = utype;\r\n    this.getDatas();\r\n    this.getUserinfoList();\r\n  },\r\n  methods: {\r\n    handleAdd() {\r\n      //弹出新增窗口\r\n      this.formVisible = true;\r\n      this.ureadonly = false;\r\n      this.rules = this.addrules;\r\n      this.formData = {};\r\n    },\r\n    handleEdit(index, row) {\r\n      //打开修改界面\r\n      this.formVisible = true;\r\n      this.ureadonly = true;\r\n      this.rules = this.modifyrules;\r\n      this.formData = JSON.parse(JSON.stringify(row));\r\n    },\r\n\r\n  handleResult(index, row) {\r\n        \r\n      this.formData = JSON.parse(JSON.stringify(row));\r\n      window.location.href = '/timuResult?dtid=' + this.formData.id;\r\n    },\r\n    \r\n    handlePj(index, row) {\r\n      this.formData = JSON.parse(JSON.stringify(row));\r\n      let sj = this.formData.seconds;\r\n\r\n      var user = sessionStorage.getItem(\"currentUser\");\r\n      user = JSON.parse(user);\r\n\r\n      let para = {};\r\n\r\n      let url =\r\n        base + \"/xsksitem/checkks?uid=\" + user.id + \"&ksid=\" + this.formData.id;\r\n\r\n      request.post(url, para).then((res) => {\r\n        let code = res.code;\r\n        if (code == 200) {\r\n          this.$router.push(\"/datiAll?ksid=\" + this.formData.id);\r\n        } else {\r\n          let msg = res.msg;\r\n          this.$message({\r\n            message: msg,\r\n            type: \"error\",\r\n            offset: 320,\r\n          });\r\n        }\r\n      });\r\n    },\r\n\r\n    save() {\r\n      if (this.formData.id != null) {\r\n        //如果有ID就修改数据\r\n        this.$refs[\"formDataRef\"].validate((valid) => {\r\n          if (valid) {\r\n            let url = base + \"/kcinfo/update?id=\" + this.formData.id;\r\n            this.btnLoading = true;\r\n            request.post(url, this.formData).then((res) => {\r\n              if (res.code == 200) {\r\n                this.btnLoading = false;\r\n                this.formVisible = false;\r\n                this.$message({\r\n                  message: \"操作成功\",\r\n                  type: \"success\",\r\n                  offset: 320,\r\n                });\r\n                window.location.href = \"kcinfo\";\r\n              } else {\r\n                this.$message({\r\n                  message: \"服务器错误\",\r\n                  type: \"error\",\r\n                  offset: 320,\r\n                });\r\n              }\r\n            });\r\n          }\r\n        });\r\n      } else {\r\n        //如果没有ID就新增数据\r\n        this.$refs[\"formDataRef\"].validate((valid) => {\r\n          if (valid) {\r\n            let url = base + \"/kcinfo/add\";\r\n            //var user = sessionStorage.getItem(\"currentUser\");\r\n            //user = JSON.parse(user);\r\n            this.btnLoading = true;\r\n            request.post(url, this.formData).then((res) => {\r\n              console.log(res);\r\n              if (res.code == 200) {\r\n                this.$message({\r\n                  message: \"操作成功\",\r\n                  type: \"success\",\r\n                  offset: 320,\r\n                });\r\n                this.btnLoading = false;\r\n                this.formVisible = false;\r\n                window.location.href = \"kcinfo\";\r\n              } else {\r\n                this.$message({\r\n                  message: \"服务器错误\",\r\n                  type: \"error\",\r\n                  offset: 320,\r\n                });\r\n              }\r\n            });\r\n          }\r\n        });\r\n      }\r\n    },\r\n    handleDelete(index, row) {\r\n      this.$confirm(\"确认删除该记录吗?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.listLoading = true;\r\n          let url = base + \"/kcinfo/del?id=\" + row.id;\r\n          request.post(url).then((res) => {\r\n            this.listLoading = false;\r\n            //NProgress.done();\r\n            this.$message({\r\n              message: \"删除成功\",\r\n              type: \"success\",\r\n              offset: 320,\r\n            });\r\n            window.location.href = \"kcinfo\";\r\n          });\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.page.currentPage = val;\r\n      this.getDatas();\r\n    },\r\n    query() {\r\n      //查询\r\n      this.getDatas();\r\n    },\r\n    //获取列表数据\r\n    getDatas() {\r\n      //var user = sessionStorage.getItem(\"currentUser\");\r\n      //user = JSON.parse(user);\r\n      let para = {\r\n        name: this.filters.name,\r\n        tid: this.filters.tid,\r\n      };\r\n\r\n      if (this.utype == \"老师\") {\r\n        var user = sessionStorage.getItem(\"currentUser\");\r\n        user = JSON.parse(user);\r\n        para = {\r\n          name: this.filters.name,\r\n          tid: user.id,\r\n        };\r\n      }\r\n\r\n      if (this.utype == \"学生\") {\r\n        var user = sessionStorage.getItem(\"currentUser\");\r\n        user = JSON.parse(user);\r\n        para = {\r\n          name: this.filters.name,\r\n          bid: user.bid,\r\n        };\r\n      }\r\n\r\n\r\n      this.listLoading = true;\r\n      let url = base + \"/kcinfo/list?currentPage=\" + this.page.currentPage;\r\n      console.log(url);\r\n      request.post(url, para).then((res) => {\r\n        console.log(res);\r\n        if (res.resdata.length > 0) {\r\n          this.isPage = true;\r\n        } else {\r\n          this.isPage = false;\r\n        }\r\n        this.page.totalCount = res.count;\r\n        this.datalist = res.resdata;\r\n        this.listLoading = false;\r\n      });\r\n    },\r\n    getUserinfoList() {\r\n      let para = {};\r\n      this.listLoading = true;\r\n      let url = base + \"/kcinfo/listUserinfoAll\";\r\n      request.post(url, para).then((res) => {\r\n        this.userinfoList = res.resdata;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}