gin-left: 10px\"\r\n    >\r\n      <el-form :inline=\"true\" :model=\"filters\">\r\n        <el-form-item label=\"课程\" prop=\"kcid\">\r\n          <el-select\r\n            v-model=\"filters.kcid\"\r\n            placeholder=\"请选择\"\r\n           \r\n          >\r\n            <el-option label=\"请选择\" value=\"0\"> </el-option>\r\n            <el-option\r\n              v-for=\"item in kcinfoList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n       \r\n        <el-form-item>\r\n          <el-button type=\"primary\" size=\"small\" @click=\"query\">查询</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" size=\"small\" @click=\"handleAdd\"\r\n            >新增</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-col>\r\n    <!--列表-->\r\n    <el-table\r\n      :data=\"datalist\"\r\n      border\r\n      stripe\r\n      v-loading=\"listLoading\"\r\n      highlight-current-row\r\n      max-height=\"600\"\r\n      size=\"small\"\r\n      style=\"width: 100%\"\r\n    >\r\n         <el-table-column\r\n        prop=\"title\"\r\n        align=\"center\"\r\n        label=\"课程\"\r\n        min-width=\"20%\"\r\n      >\r\n        <template #default=\"scope\"> {{ scope.row.kcinfoVO.name }}</template>\r\n      </el-table-column>\r\n\r\n       <el-table-column\r\n        prop=\"title\"\r\n        align=\"center\"\r\n        label=\"老师\"\r\n        min-width=\"20%\"\r\n      >\r\n        <template #default=\"scope\"> {{ scope.row.kcinfoVO.userinfoVO.name }}</template>\r\n      </el-table-column>\r\n\r\n\r\n          <el-table-column\r\n        prop=\"title\"\r\n        align=\"center\"\r\n        label=\"班级\"\r\n        min-width=\"20%\"\r\n      >\r\n        <template #default=\"scope\"> {{ scope.row.bjinfoVO.name }}</template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"300\">\r\n        <template #default=\"scope\">\r\n         \r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.$index, scope.row)\"\r\n            >删除</el-button\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"page.currentPage\"\r\n      :page-size=\"page.pageSize\"\r\n      background\r\n      layout=\"total, prev, pager, next, jumper\"\r\n      :total=\"page.totalCount\"\r\n      style=\"float: right; margin: 10px 20px 0 0\"\r\n    >\r\n    </el-pagination>\r\n    <!--表单-->\r\n    <el-dialog\r\n      title=\"课程班级信息\"\r\n      v-model=\"formVisible\"\r\n      width=\"40%\"\r\n      :close-on-click-modal=\"false\"\r\n    >\r\n      <el-form\r\n        :model=\"formData\"\r\n        ref=\"formDataRef\"\r\n        :rules=\"rules\"\r\n        label-width=\"100px\"\r\n      >\r\n        <el-form-item label=\"课程\" prop=\"kcid\">\r\n          <el-select\r\n            v-model=\"formData.kcid\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 50%\"\r\n          >\r\n            <el-option label=\"请选择\" value=\"0\"> </el-option>\r\n            <el-option\r\n              v-for=\"item in kcinfoList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name + ' ' + item.userinfoVO.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"班级\" prop=\"bjid\">\r\n          <el-select\r\n            v-model=\"formData.bjid\"\r\n            placeholder=\"请选择\"\r\n            style=\"width: 50%\"\r\n          >\r\n            <el-option label=\"请选择\" value=\"0\"> </el-option>\r\n            <el-option\r\n              v-for=\"item in bjinfoList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"formVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" :loading=\"btnLoading\" @click=\"save\"\r\n            >确 定</el-button\r\n          >\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </section>\r\n</template>\r\n<script>\r\nimport request, { base } from \"../../../utils/http\";\r\nexport default {\r\n  name: \"Kcbjinfo\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      filters: {\r\n        //列表查询参数\r\n        kcid: \"\",\r\n        bjid: \"\",\r\n      },\r\n      page: {\r\n        currentPage: 1, // 当前页\r\n        pageSize: 8, // 每页显示条目个数\r\n        totalCount: 0, // 总条目数\r\n      },\r\n      kcinfoList: \"\",\r\n      bjinfoList: \"\",\r\n      listLoading: false, //列表加载状态\r\n      btnLoading: false, //保存按钮加载状态\r\n      datalist: [], //表格数据\r\n      formVisible: false, //表单是否可见，默认不可见\r\n      formData: {}, //表单数据\r\n      ureadonly: false,\r\n      rules: {},\r\n      addrules: {\r\n        kcid: [{ required: true, message: \"请输入课程\", trigger: \"blur\" }],\r\n        bjid: [{ required: true, message: \"请输入班级\", trigger: \"blur\" }],\r\n      },\r\n      modifyrules: {\r\n        kcid: [{ required: true, message: \"请输入课程\", trigger: \"blur\" }],\r\n        bjid: [{ required: true, message: \"请输入班级\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getDatas();\r\n    this.getKcinfoList();\r\n    this.getBjinfoList();\r\n  },\r\n  methods: {\r\n    handleAdd() {\r\n      //弹出新增窗口\r\n      this.formVisible = true;\r\n      this.ureadonly = false;\r\n      this.rules = this.addrules;\r\n      this.formData = {};\r\n    },\r\n    handleEdit(index, row) {\r\n      //打开修改界面\r\n      this.formVisible = true;\r\n      this.ureadonly = true;\r\n      this.rules = this.modifyrules;\r\n      this.formData = JSON.parse(JSON.stringify(row));\r\n    },\r\n    save() {\r\n      if (this.formData.id != null) {\r\n        //如果有ID就修改数据\r\n        this.$refs[\"formDataRef\"].validate((valid) => {\r\n          if (valid) {\r\n            let url = base + \"/kcbjinfo/update?id=\" + this.formData.id;\r\n            this.btnLoading = true;\r\n            request.post(url, this.formData).then((res) => {\r\n              if (res.code == 200) {\r\n                this.btnLoading = false;\r\n                this.formVisible = false;\r\n                this.$message({\r\n                  message: \"操作成功\",\r\n                  type: \"success\",\r\n                  offset: 320,\r\n                });\r\n                window.location.href = 'kcbjinfo';\r\n              } else {\r\n                this.$message({\r\n                  message: \"服务器错误\",\r\n                  type: \"error\",\r\n                  offset: 320,\r\n                });\r\n              }\r\n            });\r\n          }\r\n        });\r\n      } else {\r\n        //如果没有ID就新增数据\r\n        this.$refs[\"formDataRef\"].validate((valid) => {\r\n          if (valid) {\r\n            let url = base + \"/kcbjinfo/add\";\r\n            //var user = sessionStorage.getItem(\"currentUser\");\r\n            //user = JSON.parse(user);\r\n            this.btnLoading = true;\r\n            request.post(url, this.formData).then((res) => {\r\n              console.log(res);\r\n              if (res.code == 200) {\r\n                this.$message({\r\n                  message: \"操作成功\",\r\n                  type: \"success\",\r\n                  offset: 320,\r\n                });\r\n                this.btnLoading = false;\r\n                this.formVisible = false;\r\n                window.location.href = 'kcbjinfo';\r\n              } else {\r\n                   this.btnLoading = false;\r\n                   let msg = res.msg;\r\n                this.$message({\r\n                  message:msg,\r\n                  type: \"error\",\r\n                  offset: 320,\r\n                });\r\n              }\r\n            });\r\n          }\r\n        });\r\n      }\r\n    },\r\n    handleDelete(index, row) {\r\n      this.$confirm(\"确认删除该记录吗?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.listLoading = true;\r\n          let url = base + \"/kcbjinfo/del?id=\" + row.id;\r\n          request.post(url).then((res) => {\r\n            this.listLoading = false;\r\n            //NProgress.done();\r\n            this.$message({\r\n              message: \"删除成功\",\r\n              type: \"success\",\r\n              offset: 320,\r\n            });\r\n             window.location.href = 'kcbjinfo';\r\n          });\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.page.currentPage = val;\r\n      this.getDatas();\r\n    },\r\n    query() {\r\n      //查询\r\n      this.getDatas();\r\n    },\r\n    //获取列表数据\r\n    getDatas() {\r\n      //var user = sessionStorage.getItem(\"currentUser\");\r\n      //user = JSON.parse(user);\r\n      let para = {\r\n        kcid: this.filters.kcid,\r\n        bjid: this.filters.bjid,\r\n      };\r\n      this.listLoading = true;\r\n      let url = base + \"/kcbjinfo/list?currentPage=\" + this.page.currentPage;\r\n      console.log(url);\r\n      request.post(url, para).then((res) => {\r\n        console.log(res);\r\n        if (res.resdata.length > 0) {\r\n          this.isPage = true;\r\n        } else {\r\n          this.isPage = false;\r\n        }\r\n        this.page.totalCount = res.count;\r\n        this.datalist = res.resdata;\r\n        this.listLoading = false;\r\n      });\r\n    },\r\n    getKcinfoList() {\r\n      let para = {};\r\n      this.listLoading = true;\r\n      let url = base + \"/kcbjinfo/listKcinfoAll\";\r\n      request.post(url, para).then((res) => {\r\n        this.kcinfoList = res.resdata;\r\n      });\r\n    },\r\n    getBjinfoList() {\r\n      let para = {};\r\n      this.listLoading = true;\r\n      let url = base + \"/kcbjinfo/listBjinfoAll\";\r\n      request.post(url, para).then((res) => {\r\n        this.bjinfoList = res.resdata;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}