>\r\n    <ul>\r\n      <li class=\"li-page\" @click=\"goPrePage\">&laquo;</li>\r\n      <li v-for=\"(i, index) in showPageBtn\" :key=\"index\"\r\n        :class=\"{active: i === currentPage, pointer: i, hover: i && i !== currentPage}\" @click=\"pageOffset(i)\">\r\n        <a v-if=\"i\">{{i}}</a>\r\n        <a v-else>···</a>\r\n      </li>\r\n      <li class=\"li-page\" @click=\"goNextPage\">&raquo;</li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: \"pagination\",\r\n    props: {\r\n      num: Number,\r\n      limit: Number\r\n    },\r\n    data: () => ({\r\n      offset: 0\r\n    }),\r\n    computed: {\r\n      prePage() {\r\n        return this.offset !== 0 && this.num;\r\n      },\r\n      nextPage() {\r\n        return (this.offset + this.limit < this.num) && this.num\r\n      },\r\n      totalPage() {\r\n        return Math.ceil(this.num / this.limit)\r\n      },\r\n      currentPage() {\r\n        return Math.ceil(this.offset / this.limit) + 1\r\n      },\r\n      showPageBtn() {\r\n        const pageNum = this.totalPage;\r\n        const index = this.currentPage;\r\n        if (pageNum <= 5) return [...new Array(pageNum)].map((v, i) => i + 1);\r\n        if (index <= 2) return [1, 2, 3, 0, pageNum];\r\n        if (index >= pageNum - 1) return [1, 0, pageNum - 2, pageNum - 1, pageNum];\r\n        if (index === 3) return [1, 2, 3, 4, 0, pageNum];\r\n        if (index === pageNum - 2) return [1, 0, pageNum - 3, pageNum - 2, pageNum - 1, pageNum];\r\n        return [1, 0, index - 1, index, index + 1, 0, pageNum]\r\n      }\r\n    },\r\n    methods: {\r\n      pageOffset(i) {\r\n      \r\n        if (i === 0 || i === this.currentPage) return;\r\n        this.offset = (i - 1) * this.limit;\r\n \r\n        this.$emit('getList', this.offset);\r\n      },\r\n      goPrePage() {\r\n        if (!this.prePage) {\r\n          //alert(\"这里是第一页呀，亲!\");\r\n          return;\r\n        }\r\n        this.offset -= this.limit;\r\n        this.$emit('getList', this.offset);\r\n      },\r\n      goNextPage() {\r\n        if (!this.nextPage) {\r\n         // alert(\"已经是最后一页了呢~\");\r\n          return;\r\n        }\r\n        this.offset += this.limit;\r\n        this.$emit('getList', this.offset);\r\n      }\r\n    }\r\n  }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  .li-page {\r\n    line-height: 1.5;\r\n    cursor: pointer;\r\n    color: #c2c4cc;\r\n    background-color: #f4f4f5;\r\n  }\r\n\r\n  .active {\r\n    border-color: #15c288;\r\n    background-color: #15c288;\r\n  }\r\n\r\n  .pointer {\r\n    cursor: pointer;\r\n  }\r\n\r\n  .hover {\r\n    color: #FFF;\r\n    background-color: #f4f4f5;\r\n  }\r\n\r\n  .active a {\r\n    color: #FFF;\r\n  }\r\n\r\n  .page-wrap {\r\n    font-size: 13px;\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .page-wrap ul {\r\n    display: inline-block;\r\n    list-style: none;\r\n    overflow: hidden;\r\n    padding-inline-start: 0px;\r\n  }\r\n\r\n  .page-wrap li {\r\n    float: left;\r\n    color: #c2c4cc;\r\n    padding: 3px 10px;\r\n    margin: 0 5px;\r\n    border-radius: 3px;\r\n    user-select: none;\r\n    border: 1px solid transparent;\r\n  }\r\n\r\n</style>"]}]}